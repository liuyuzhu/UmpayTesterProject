$(function(){
	$('#entry').click(function(){
	if($('#adminName').val()==''){
			$('.mask,.dialog').show();
			$('.dialog .dialog-bd p').html('请输入管理员账户');
			}
		if($('#adminName').val()=='admin'){
			$('.mask,.dialog').show();
			$('.dialog .dialog-bd p').html('请输入管理员密码');
			if($('#adminPwd').val()=='admin'){
			$('.mask,.dialog').hide();
			location.href='index.html';
			
			}
		}
	});
});
